{
    "extends": "../tslint.json",
    "rules": {
        "align": [true, "parameters"],
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "max-line-length": [
            true,
            200
        ]
    }
}
